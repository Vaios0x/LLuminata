{
  "defaults": {
    "timeout": 1000,
    "wait": 1000,
    "standard": "WCAG2AA",
    "level": "error",
    "hideElements": "#pa11y-ignore",
    "chromeLaunchConfig": {
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox",
